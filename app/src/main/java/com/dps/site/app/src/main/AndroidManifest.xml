<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dps.site">

    <application
        android:allowBackup="true"
        android:label="DPS Site Daily Register"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">

        <activity android:name=".DateTimeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".DashboardActivity"/>
        <activity android:name=".MaterialTypeActivity"/>
        <activity android:name=".SupplierActivity"/>
        <activity android:name=".QuantityActivity"/>
        <activity android:name=".CheckedByActivity"/>
        <activity android:name=".VehicleActivity"/>
        <activity android:name=".RemarksActivity"/>

    </application>
</manifest>
